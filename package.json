{
  "name": "HeliSwap-yield-farming",
  "version": "1.0.0",
  "description": "HeliSwap Yield Farming Contracts",
  "main": "index.js",
  "scripts": {
    "deployFactory": "npx hardhat deployFactory --network testnet",
    "deployCampaign": "npx hardhat deployCampaign --network testnet --token 0x04FB21cf2e0D65C10d4d8C029fbEFa60E69EFEa4 --factory 0x000000000000000000000000000000000044fd18 --owner 0x61c90019e9fb0d95cbd39cb68e0b4f217526e2da",
    "enableReward": "npx hardhat enableReward --network testnet --campaign 0x0000000000000000000000000000000000143549 --reward 0x00000000000000000000000000000000000f7e89 --duration 86400 --hts false",
    "sendReward": "npx hardhat sendReward --network testnet --campaign 0x0000000000000000000000000000000000143549 --reward 0x00000000000000000000000000000000000f7e89 --amount 100000000",
    "associateToken": "npx hardhat associateToken --network testnet --accountid 0.0.1323559 --pk --tokenid 0.0.127877",
    "approveToken": "npx hardhat approveToken --network testnet --account 0.0.47849515 --pk <PK> --spender 0.0.534359 --token 0.0.34741685 --amount 10000000000000000000000000000000000",
    "setupHbarCampaign": "npx hardhat setupHbarCampaign --network testnet --campaign 0x000000000000000000000000000000000044fD1b --hbaramount 1000 --duration 31536000",
    "setupHTSCampaign": "npx hardhat setupHTSCampaign --network testnet --campaign 0x00000000000000000000000000000000003dbf2A --reward 0x0000000000000000000000000000000000002023 --amount 1400000000000 --duration 1740990",
    "setupMultiRewardsCampaign": "npx hardhat setupMultiRewardsCampaign --network testnet --factory 0x0000000000000000000000000000000002dfa29c --token 0x0a06676Eb5144ad5451bE2Dec256dfeb07f20eb4 --duration 86400",
    "campaignInfo": "npx hardhat campaignInfo --network testnet --campaign 0x000000000000000000000000000000000016a987",
    "setDuration": "npx hardhat setDuration --network testnet --campaign 0x000000000000000000000000000000000016a987 --token 0x0000000000000000000000000000000000107d76 --duration 300",
    "extendCampaign": "npx hardhat extendCampaign --network testnet --campaign 0x000000000000000000000000000000000016a987 --token 0x0000000000000000000000000000000000107d76 --reward 12374961000000 --duration 1263425"
  },
  "repository": "https://github.com/LimeChain/HeliSwap-yield-farming",
  "author": "Martin Dobrev",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@hashgraph/hardhat-hethers": "^1.0.2",
    "@hashgraph/hethers": "^1.2.1",
    "@hashgraph/sdk": "^2.17.1",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.1",
    "hardhat-typechain": "^0.3.3",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0",
    "whbar": "github:LimeChain/whbar"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3"
  }
}
