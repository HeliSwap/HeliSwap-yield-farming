{
  "name": "HeliSwap-yield-farming",
  "version": "1.0.0",
  "description": "HeliSwap Yield Farming Contracts",
  "main": "index.js",
  "scripts": {
    "deployFactory": "npx hardhat deployFactory --network testnet",
    "deployCampaign": "npx hardhat deployCampaign --network testnet --factory 0x0000000000000000000000000000000002dd972f --owner 0x0000000000000000000000000000000002da202b --token 0x978264868aA0730718FBe0A3CCCF8C09f6eb74C3",
    "enableReward": "npx hardhat enableReward --network testnet --campaign 0x0000000000000000000000000000000002dcD66B --reward 0x0000000000000000000000000000000002dce0b7 --duration 2629743",
    "sendReward": "npx hardhat sendReward --network testnet --campaign 0x0000000000000000000000000000000002dcD66B --reward 0x0000000000000000000000000000000002dce0b7 --amount 200000000000",
    "associateToken": "npx hardhat associateToken --network testnet --accountid 0.0.47849515 --pk <PK> --tokenid 0.0.48029879",
    "approveToken": "npx hardhat approveToken --network testnet --accountid 0.0.47849515 --pk <PK> --spenderaccountid 0.0.48027243 --tokenid 0.0.48029879 --amount 200000000000",
    "setupHbarCampaign": "npx hardhat setupHbarCampaign --network testnet --factory 0x0000000000000000000000000000000002de1b92 --token 0x978264868aA0730718FBe0A3CCCF8C09f6eb74C3 --hbaramount 100 --duration 1000",
    "campaignInfo": "npx hardhat campaign-info --network testnet --campaign 0x0000000000000000000000000000000002dDB292 --wallet-address 0x0000000000000000000000000000000002099E42 --index 0 --decimals 8"
  },
  "repository": "https://github.com/LimeChain/HeliSwap-yield-farming",
  "author": "Martin Dobrev",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@hashgraph/hardhat-hethers": "^1.0.2",
    "@hashgraph/hethers": "^1.2.1",
    "@hashgraph/sdk": "^2.17.1",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.1",
    "hardhat-typechain": "^0.3.3",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0",
    "whbar": "github:LimeChain/whbar"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3"
  }
}
