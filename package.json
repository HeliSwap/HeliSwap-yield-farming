{
  "name": "HeliSwap-yield-farming",
  "version": "1.0.0",
  "description": "HeliSwap Yield Farming Contracts",
  "main": "index.js",
  "scripts": {
    "deployFactory": "npx hardhat deployFactory",
    "deployMultirewards": "npx hardhat deployMultirewards --factoryaddress 0x0000000000000000000000000000000002dca44b --owner 0x0000000000000000000000000000000002134dEd --stakingtoken 0x978264868aA0730718FBe0A3CCCF8C09f6eb74C3",
    "addReward": "npx hardhat addReward --contractaddress 0x0000000000000000000000000000000002DCa44E --rewardaddress 0x0000000000000000000000000000000002be8c90 --rewarddistributor 0x0000000000000000000000000000000002099E42 --rewardduration 31536000",
    "sendReward": "npx hardhat sendReward --contractaddress 0x0000000000000000000000000000000002DCa44E --rewardaddress 0x0000000000000000000000000000000002be8c90 --rewardamount 100 --rewarddecimals 8",
    "associateToken": "npx hardhat associateToken --accountid 0.0.34402467 --pk <PRIVATE_KEY> --tokenid 0.0.34741650",
    "approveToken": "npx hardhat approveToken --accountid 0.0.34184770 --pk <PRIVATE_KEY> --spenderaccountid 0.0.48014414 --tokenid 0.0.46042256 --amount 10000000000"
  },
  "repository": "https://github.com/LimeChain/HeliSwap-yield-farming",
  "author": "Martin Dobrev",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@hashgraph/hardhat-hethers": "^1.0.0",
    "@hashgraph/hethers": "^1.2.0",
    "@hashgraph/sdk": "^2.17.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.7.21",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  }
}
